{
  "name": "photographerlagbe",
  "version": "1.0.0",
  "description": "A platform to find photographers based on preferences",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "cross-env PORT=5001 nodemon server.js",
    "client": "cross-env PORT=3001 npm start --prefix client",
    "dev": "concurrently --kill-others \"npm run server\" \"npm run client\" --shell cmd.exe",
    "install-client": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run install-client && npm run build"
  },
  "keywords": [
    "photography",
    "booking",
    "mern",
    "react",
    "node"
  ],
  "author": "PhotographerLagbe Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.40.0",
    "cors": "^2.8.5",
    "cross-env": "^10.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  }
}
